{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch App",
			"type": "node",
			"request": "launch",
			"protocol": "inspector",
			"cwd": "${workspaceRoot}",
			"runtimeExecutable": "npm",
			"windows": {
				"runtimeExecutable": "npm.cmd"
			},
			"runtimeArgs": [
				"run-script",
				"debug"
			],
			"port": 33295,
			"env": {
				"NODE_ENV": "development",
				"INPUT_SAMPLESTRING": "help"
			}
		},
		{
			"name": "Launch Tests",
			"type": "node",
			"request": "launch",
			"protocol": "inspector",
			"cwd": "${workspaceRoot}",
			"runtimeExecutable": "npm",
			"windows": {
				"runtimeExecutable": "npm.cmd"
			},
			"runtimeArgs": [
				"run-script",
				"debug-test"
			],
			"timeout": 50000,
			"port": 33295,
			"env": {
				"NODE_ENV": "test"
			}
		},
	]
}