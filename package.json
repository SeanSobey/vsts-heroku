{
	"name": "vsts-heroku-cli",
	"version": "1.0.2",
	"description": "Heroku CLI Tools for VSTS.",
	"main": "dist/index.js",
	"scripts": {
		"start": "node dist/index.js",
		"prebuild": "del-cli ./dist",
		"build": "tsc",
		"test": "mocha --opts ./mocha.opts dist/**/*.spec.js",
		"debug": "node --inspect-brk=33295 --nolazy dist/index.js",
		"debug-test": "node --inspect-brk=33295 --nolazy node_modules/mocha/bin/_mocha --opts ./mocha.opts dist/**/*.spec.js",
		"watch-build": "tsc --watch",
		"watch-test": "mocha --watch --opts ./mocha.opts dist/**/*.spec.js",
		"tfx-package": "tfx extension create --manifest-globs vss-extension.json --rev-version",
		"tfx-publish": "tfx extension publish --manifest-globs vss-extension.json"
	},
	"bugs": {
		"url": "https://github.com/SeanSobey/vsts-heroku-cli/issues",
		"email": "sean.m.sobey@gmail.com"
	},
	"author": {
		"name": "Sean Sobey",
		"email": "sean.m.sobey@gmail.com"
	},
	"homepage": "https://github.com/SeanSobey/vsts-heroku-cli",
	"repository": {
		"type": "git",
		"url": "https://github.com/SeanSobey/vsts-heroku-cli.git"
	},
	"readme": "README.md",
	"license": "LICENSE",
	"dependencies": {
		"heroku-cli": "^6.15.22",
		"tslib": "^1.9.0",
		"vsts-task-lib": "^2.1.0"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.48",
		"@types/node": "^9.4.0",
		"@types/q": "^1.0.7",
		"del-cli": "^1.1.0",
		"mocha": "^5.0.0",
		"tfx-cli": "^0.5.4",
		"tslint": "^5.9.1",
		"tslint-divid": "^1.3.0",
		"tslint-eslint-rules": "^4.1.1",
		"tslint-immutable": "^4.5.1",
		"typescript": "^2.7.1"
	}
}
